<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.phonegap.admob.test" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>com.phonegap.admob.test</name>
    <icon src="icon.png" />
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>AdMob</name>
    <description>Hello World sample application that responds to the deviceready event.</description>
    <author email="vcshing2005@gmail.com" href="#">Skyler Explorer</author>
    <content src="index.html" />
    <icon src="res/icons/appFeel-512.png" />
    <icon gap:platform="ios" height="60" src="icon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="icon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="76" src="icon-76.png" width="76" />
    <icon height="152" src="icon-76@2x.png" width="152" />
    <icon gap:platform="ios" height="40" src="icon-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="icon-40@2x.png" width="80" />
    <icon gap:platform="ios" height="57" src="icon.png" width="57" />
    <icon gap:platform="ios" height="114" src="icon@2x.png" width="114" />
    <icon gap:platform="ios" height="72" src="icon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="icon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="29" src="icon-Small.png" width="29" />
    <icon gap:platform="ios" height="58" src="icon-Small@2x.png" width="58" />
    <icon gap:platform="ios" height="50" src="icon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="icon-Small-50@2x.png" width="100" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="LogLevel" value="VERBOSE" />
    <preference name="debuggable" value="true" />
</widget>
